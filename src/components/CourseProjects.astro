---
import { labels } from "../i18n";
import ProjectsRow from "./ProjectsRow.astro";

const { lang = "en" } = Astro.props;
const t = labels[lang];
---

<ProjectsRow
  id="course-projects"
  title={t.courseProjects.title}
  subtitle={t.courseProjects.subtitle}
  hint={t.courseProjects.hint}
  items={t.courseProjects.items ?? []}
  openRepoText={t.courseProjects.openRepo}
  reverse={true}
/>
